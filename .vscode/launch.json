{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:9999/app/admin/#/",
            "sourceMaps": false,
            "webRoot": "${workspaceRoot}/webapp"
        },
        {
            "name": "Attach",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "sourceMaps": false,
            "webRoot": "${workspaceRoot}/webapp"
        }
    ]
}